//Write a program that asks the user to enter a two digit number, then prints the number with its digits reversed
#include <stdio.h>

int main() {
	int number1 = 0; 
	int number2 = 0; 
	printf("Enter a two-digit number: "); 
	 
	if (scanf_s("%1d%1d", &number1, &number2) != EOF) {
		printf("The reversal is: %d%d", number2, number1);
		return 0; 
	}
	else
		printf("error"); 
}

